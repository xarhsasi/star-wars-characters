# General
# Redis
# ------------------------------------------------------------------------------
REDIS_URL=redis://redis:6379/0
CELERY_BROKER_URL=${REDIS_URL}

# Security
# ------------------------------------------------------------------------------
CORS_ORIGINS='["*"]'

# Logging
# ------------------------------------------------------------------------------
LOGGING_LEVEL=DEBUG
LOGGING_FORMAT=plain

# Datadog
# ------------------------------------------------------------------------------
DD_ENABLED=false

# SWAGGER
# ------------------------------------------------------------------------------
ENABLE_SWAGGER=true

# Authentication
# ------------------------------------------------------------------------------
JWT_ALGORITHM=HS256
JWT_SECRET=secret
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=60
